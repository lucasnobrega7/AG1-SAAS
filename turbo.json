{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "globalEnv": [
    "NODE_ENV",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "REDIS_URL",
    "SUPABASE_JWT_SECRET",
    "SUPABASE_SERVICE_ROLE",
    "CLERK_SECRET_KEY",
    "ZAPI_BASE_URL",
    "ZAPI_INSTANCE_ID",
    "ZAPI_TOKEN",
    "ZAPI_CLIENT_TOKEN",
    "WEBHOOK_URL",
    "RAILWAY_TOKEN",
    "SUPABASE_URL",
    "SUPABASE_ANON_KEY",
    "API_N8N",
    "OPENAI_API_KEY",
    "ANTHROPIC_API_KEY",
    "COHERE_API_KEY",
    "API_KEY",
    "POSTHOG_API_KEY",
    "SENTRY_DSN",
    "LANGSMITH_TRACING",
    "LANGSMITH_ENDPOINT",
    "LANGSMITH_API_KEY",
    "LANGSMITH_PROJECT",
    "LANGCHAIN_PROJECT_KEY",
    "RESEND_API_KEY"
  ],
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"],
      "env": [
        "REDIS_URL",
        "SUPABASE_JWT_SECRET",
        "SUPABASE_SERVICE_ROLE",
        "CLERK_SECRET_KEY",
        "ZAPI_BASE_URL",
        "ZAPI_INSTANCE_ID",
        "ZAPI_TOKEN",
        "ZAPI_CLIENT_TOKEN",
        "WEBHOOK_URL",
        "RAILWAY_TOKEN",
        "SUPABASE_URL",
        "SUPABASE_ANON_KEY",
        "API_N8N",
        "OPENAI_API_KEY",
        "ANTHROPIC_API_KEY",
        "COHERE_API_KEY",
        "API_KEY",
        "POSTHOG_API_KEY",
        "SENTRY_DSN",
        "LANGSMITH_TRACING",
        "LANGSMITH_ENDPOINT",
        "LANGSMITH_API_KEY",
        "LANGSMITH_PROJECT",
        "LANGCHAIN_PROJECT_KEY",
        "RESEND_API_KEY"
      ]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
